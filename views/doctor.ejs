<body>

    <h1>Doctor</h1>

    <form action="/doctor" method="POST">
        <input type="text" placeholder="Name" name="name">
        <input type="number" placeholder="Age" name="age">
        <input type="text" placeholder="Gender" name="gender">
        <input type="text" placeholder="consultation-hour" name="consultation">
        
        <input type="text" placeholder="Password" name="password">
        
        <input type="email" placeholder="Email" name="email">
        <input type="text" placeholder="Speciality" name="speciality">
        <input type="text" placeholder="Qualification" name="qualification">
        <input type="text" placeholder="Doctor Id " name="docId">
        <input type="text" placeholder="fees" name="fees">
        
        <button type="submit">Submit</button>
        
    </form>
    
